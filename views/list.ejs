<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="styles.css">
    <title>List all task</title>
</head>

<body>
    <a href="/">home</a>
    <a href="/create">Add new developer</a>
    <a href="/delete">Delete developer</a>
    <a href="/update">update developer</a>
    <table>
        <thead>
            <tr>
                <td>id</td>
                <td>first name</td>
                <td>last name</td>
                <td>level</td>
                <td>state</td>
                <td>suburb</td>
                <td>street</td>
                <td>unit</td>
            </tr>
        </thead>
        <% for(var i =0;i< db.length;i++){ %>
            <tr>
                <td><%= db[i]._id%></td>
                <td><%= db[i].name.firstName%></td>
                <td><%= db[i].name.lastName%></td>
                <td><%= db[i].level %></td>
                <td><%= db[i].address.state%></td>
                <td><%= db[i].address.suburb %></td>
                <td><%= db[i].address.street %></td>
                <td><%= db[i].address.unit %></td>
            </tr>

            <% } %>
        <tbody>

        </tbody>
    </table>
</body>

</html>




